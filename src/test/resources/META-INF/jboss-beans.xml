<?xml version="1.0" encoding="UTF-8"?>
<deployment xmlns="urn:jboss:bean-deployer:2.0">
   
   <bean name="translator-ibis-template" class="org.teiid.templates.TranslatorDeploymentTemplate">
      <property name="info"><inject bean="translator-ibis"/></property>
      <property name="managedObjectFactory"><inject bean="ManagedObjectFactory"/></property>
   </bean>
    
   <bean name="translator-ibis" class="org.teiid.templates.TranslatorTemplateInfo">
      <constructor factoryMethod="createTemplateInfo">
         <factory bean="TranslatorDeploymentTemplateInfoFactory"/>
         <parameter class="java.lang.Class">org.teiid.templates.TranslatorTemplateInfo</parameter>
         <parameter class="java.lang.Class">org.jboss.teiid.translator.ibis.IbisExecutionFactory</parameter>
         <parameter class="java.lang.String">translator-ibis</parameter>
         <parameter class="java.lang.String">ibis</parameter>         
      </constructor>
   </bean> 
   
</deployment>